{
  "short_name": "Eddy",
  "name": "Eddy: Your knowlegebase",
  "description": "Your personal knowlegebase",
  "lang": "en",
  "start_url": "index.html",
  "display": "standalone",
  "theme_color": "#b8cddd",
  "background_color": "#b8cddd",

  "icons": [
    {
      "src": "icons/192x192.png",
      "type": "image/png",
      "sizes": "192x192"
    },
    {
      "src": "icons/512x512.png",
      "type": "image/png",
      "sizes": "512x512"
    },
	{
		"src": "icons/origin.svg",
        "purpose": "maskable any",
		"sizes": "any"
    }
  ],

  "share_target": {
    "action": "index.html",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "name",
      "text": "description",
      "url": "link",
      "files": [
        {
          "name": "Text",
          "accept": "text/plain"
        },
        {
          "name": "Rich Text",
          "accept": "text/richtext"
        },
        {
          "name": "RTF Text",
          "accept": ["application/rtf", ".rtf"]
        },
        {
          "name": "HTML Web page",
          "accept": "text/html"
        },
        {
          "name": "PDF Document",
          "accept": "application/pdf"
        },
        {
          "name": "BMP Image",
          "accept": "image/bmp"
        },
        {
          "name": "GIF Image",
          "accept": "image/gif"
        },
        {
          "name": "JPEG Image",
          "accept": "image/jpeg"
        },
        {
          "name": "PNG Image",
          "accept": "image/png"
        },
        {
          "name": "SVG Image",
          "accept": "image/svg+xml"
        },
        {
          "name": "TIFF Image",
          "accept": "image/tiff"
        }
      ]
    }
  }
}
